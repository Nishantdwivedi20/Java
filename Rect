package com.company;

public class rectangle {
    int len;
    int br;
    rectangle(int len,int br ){
        this.len=len;
        this.br=br;
    }
    void area(){
        int area=len*br;
        System.out.println(area);
    }
}
class test12{
    public static void main(String[] args) {
        rectangle obj = new rectangle(5,4);
        obj.area();
    }
}
